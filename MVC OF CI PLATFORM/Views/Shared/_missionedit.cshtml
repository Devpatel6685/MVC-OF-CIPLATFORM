@model CI_PLATFORM.Entities.ViewModels.MissionAddViewModel@{    Layout = "~/Views/Admin/UserpageInAdmin.cshtml";}<head>    @*    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">    *@    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">    <link rel="stylesheet" href="~/css/missionadd.css"></head><body>    <div class="container mt-2">        <div class="col-12 bg-light border p-2">            Add        </div>        <form method="post" asp-action="AddMission" asp-controller="Admin" enctype="multipart/form-data">            <div class="container mt-2">                <div class=" p-2 border">                    <div class="col-12 d-flex flex-column my-2">                        <label for="title">Mission Title</label>                        <input type="text" id="title" class="form-control" asp-for="Title">                    </div>                    <div class="col-12 d-flex flex-column my-2">                        <label for="orgname">Mission Organisation Name</label>                        <input type="text" id="orgname" class="form-control" asp-for="OrganizationName">                    </div>                    <div class="col-12 d-flex flex-column my-2">                        <label for="orgname">Mission Short Descrition</label>                        <input type="text" id="orgname" class="form-control" asp-for="ShortDescription">                    </div>                    <div class="col-12 d-flex flex-column my-2">                        <label for="orgdetail">Mission Organisation Detail</label>                        <textarea id="orgdetail" cols="30" rows="5" class="form-control" asp-for="OrganizationDetail"></textarea>                    </div>                    <div class="col-12 d-flex flex-column my-2">                        <label for="desc">Mission Description</label>                        @*                <input type="text" id="desc" class="form-control"  asp-for="Description">                        *@                <textarea id="desc" cols="30" rows="5" class="form-control" asp-for="Description"></textarea>                    </div>                    <div class="row">                        <div class="col-6 d-flex flex-column">                            <label>Country</label>                            <select class="form-select dropdown-toggle" asp-for="CountryId" id="countryselect">                                <option value="">Select List</option>                            </select>                        </div>                        <div class="col-6 d-flex flex-column">                            <label>City</label>                            <select class="form-select dropdown-toggle" data-toggle="dropdown" asp-for="CityId" id="cityselect">                                <option value="">Select List</option>                            </select>                        </div>                    </div>                    <div class="row">                        <div class="col-sm-4 d-flex flex-column my-3">                            <label for="theme">Theme</label>                            <select class="form-select dropdown-toggle" id="themeselect" data-toggle="dropdown" asp-for="ThemeId">                                <option value="">Select List</option>                            </select>                        </div>                        <div class="col-sm-4  d-flex flex-column my-3">                            <label for="missionType">MissionType</label>                            <select class="form-select dropdown-toggle" id="avail12" data-toggle="dropdown" asp-for="MissionType" onchange="Mission()">                                <option value="0">Select Availibility</option>                                <option value="time">Time</option>                                <option value="goal">Goal</option>                            </select>                        </div>                        <div class="col-sm-4 d-flex flex-column my-3">                            <label for="seat">Total Seats</label>                            <input type="text" id="seat" class="form-control" asp-for="TotalSeats">                        </div>                    </div>                    <div class="row">                        <div class="col-md-4 d-flex flex-column">                            <label for="sdate">StartDate</label>                            <input type="date" id="sdate" class="form-control" asp-for="StartDate">                        </div>                        <div class="col-md-4 d-flex flex-column">                            <label for="rdead">Registration Deadline</label>                            <input type="date" id="rdead" class="form-control">                        </div>                        <div class="col-md-4 d-flex flex-column d-none" id="timefields">                            <label for="edate">EndDate</label>                            <input type="date" id="edate" class="form-control" asp-for="EndDate">                        </div>                        <div class="col-md-4 d-flex flex-column d-none" id="goalfields">                            <label for="seat">GoalDescription</label>                            <input type="text" id="seat" class="form-control" asp-for="GoalObjectiveText">                        </div>                    </div>                    <div class="row">                        <div class="col-6 d-flex flex-column">                            <label for="status">Status</label>                            <select class="form-select dropdown-toggle" id="status" asp-for="Status">                                <option value="1" selected>Active</option>                                <option value="0">In-Active</option>                            </select>                        </div>                        <div class="col-sm-6  d-flex flex-column">                            <label for="avail">Availability</label>                            <select class="form-select dropdown-toggle" id="avail" data-toggle="dropdown" asp-for="Availibility">                                <option value="0">Select Availibility</option>                                <option value="daily">Daily</option>                                <option value="monthly">Monthly</option>                                <option value="weekly">Weekly</option>                                <option value="week-end">Week-end</option>                            </select>                        </div>                    </div>                    <div class="col-12 d-flex flex-column">                        <label for="skills">Mission Skills</label>                        <textarea name="" id="skills" cols="30" rows="5" class="form-control"></textarea>                    </div>                    <button type="button" class="btn my-3 border  rounded-pill ">Add skill</button>                                       <div class="mb-3">                        <label asp-for="Images" class="form-label">Images</label>                        <input asp-for="Images" class="form-control" type="file" id="Images" multiple>                        <div class="mt-2 d-flex flex-wrap" id="showImage"></div>                    </div>                    <div class="mb-3">                        <label for="document" class="form-label">Documents</label>                        <input class="form-control" type="file" id="document" multiple>                    </div>                    <div class="d-flex justify-content-end">                        <button type="button" class="btn rounded-pill">Cancel</button>                        <button type="submit" class="btn rounded-pill">Submit</button>                    </div>                </div>            </div>        </form>    </div>    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>    <script src="~/js/missionedit.js"></script></body>